				framerate_factor = "1000000";
				min_framerate = "30000000";
				max_framerate = "120000000";
				step_framerate = "30000000";
				default_framerate = "60000000";
				exposure_factor = "1000000";
				min_exp_time = "14"; /* us, 2 lines */
				max_exp_time = "33334";
				step_exp_time = "1";
				default_exp_time = "8333"; /* us */
				embedded_metadata_height = "0";
			};

			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				port@0 {
					reg = <0>;
					hawk_out1: endpoint {
						vc-id = <1>;
						port-index = <0>;
						bus-width = <4>;
						remote-endpoint = <&hawk_csi_in1>;
					};
				};
			};
		};

		bmi088_a@69 {
			compatible = "bmi,bmi088";
			reg = <0x69>;
			accel_i2c_addr = <0x19>;
			accel_irq_gpio = <&tegra_main_gpio CAM_IMU_INT0 GPIO_ACTIVE_HIGH>;
			gyro_irq_gpio = <&tegra_main_gpio CAM_IMU_INT1 GPIO_ACTIVE_HIGH>;
			accel_matrix = [01 00 00 00 01 00 00 00 01];
			gyro_matrix = [01 00 00 00 01 00 00 00 01];
			gyro_reg_0x18 = <0x81>;
		};
    };

	tegra-camera-platform {
		compatible = "nvidia, tegra-camera-platform";
		num_csi_lanes = <4>;
		max_lane_speed = <20000000>;
		min_bits_per_pixel = <10>;
		vi_peak_byte_per_pixel = <2>;
		vi_bw_margin_pct = <25>;
		isp_peak_byte_per_pixel = <5>;
		isp_bw_margin_pct = <25>;

		modules {
			module0 {
				badge = "hawk_0_ar0234";
				position = "0";
				orientation = "1";
				drivernode0 {
					pcl_id = "v4l2_sensor";
					devname = "ar0234 2-0018";
					proc-device-tree = "/proc/device-tree/i2c@3180000/ar0234_a@18";
				};
			};
			module1 {
				badge = "hawk_1_ar0234";
				position = "1";
				orientation = "1";
				drivernode0 {
					pcl_id = "v4l2_sensor";
					devname = "ar0234 2-0010";
					proc-device-tree = "/proc/device-tree/i2c@3180000/ar0234_b@10";
				};
			};
		};
	};
}; 
 