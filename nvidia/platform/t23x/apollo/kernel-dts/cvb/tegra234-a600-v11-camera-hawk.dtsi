				active_w = "1920";
				active_h = "1200";
				readout_orientation = "0";
				line_length = "2448";
				inherent_gain = "1";
				pix_clk_hz = "360000000";
				serdes_pix_clk_hz = "600000000";

				gain_factor = "100";
				min_gain_val = "100"; /* dB */
				max_gain_val = "1600"; /* dB */
				step_gain_val = "1"; /* 0.1 */
				default_gain = "100";
				min_hdr_ratio = "1";
				max_hdr_ratio = "1";
				framerate_factor = "1000000";
				min_framerate = "30000000";
				max_framerate = "90000000";
				step_framerate = "30000000";
				default_framerate = "60000000";
				exposure_factor = "1000000";
				min_exp_time = "14"; /* us, 2 lines */
				max_exp_time = "33334";
				step_exp_time = "1";
				default_exp_time = "8333"; /* us */
				embedded_metadata_height = "0";
			};

			mode1 { /* mode 960x600_10Bit */
				mclk_khz = "25000";
				num_lanes = "4";
				lane_polarity = "6";
				tegra_sinterface = "serial_a";
				phy_mode = "DPHY";
				vc_id = "1";
				discontinuous_clk = "no";
				dpcm_enable = "false";
				cil_settletime = "0";
				dynamic_pixel_bit_depth = "10";
				csi_pixel_bit_depth = "10";
				mode_type = "bayer";
				pixel_phase = "grbg";

				active_w = "960";
